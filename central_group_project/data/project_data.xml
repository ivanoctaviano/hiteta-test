<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
    
        <!-- Project Stages -->
        <record id="project_project_stage_in_review" model="project.project.stage">
            <field name="sequence">16</field>
            <field name="name">In Review</field>
        </record>

        <!-- Project Sequence -->
        <record id="project_project_sequence" model="ir.sequence">
            <field name="name">Project sequence</field>
            <field name="code">project.project</field>
            <field name="prefix">PROJ-%(year)s-%(month)s-</field>
            <field eval="False" name="company_id"/>
            <field eval="1" name="number_next"/>
            <field eval="1" name="number_increment"/>
            <field name="padding">4</field>
        </record>

    </data>
</odoo>